{
    "name": "hak/skeleton",
    "description": "Payment gateway package for 2c2p one stop payment integration.",
    "authors": [
        {
            "name": "Htet Aung Khant",
            "email": "info@hakhant.tech",
            "role": "Developer"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "pestphp/pest": "^2.25"
    },
    "autoload": {
        "psr-4": {
            "Hak\\Skeletons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hak\\Skeletons\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "license": "MIT" 
}
